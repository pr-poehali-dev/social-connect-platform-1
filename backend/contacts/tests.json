{
  "tests": [
    {
      "name": "Get contacts without auth",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "Требуется авторизация"
      }
    },
    {
      "name": "Get contacts with auth",
      "method": "GET",
      "path": "/",
      "headers": {
        "X-User-Id": "1"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "contacts": "array"
      },
      "bodyMatcher": "partial"
    }
  ]
}
